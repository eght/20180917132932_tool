@Article{312409F1-1361-4974-810C-B49E54A6E826,
  author   = {Ata Allah Taleizadeh and Seyed Taghi Akhavan Niaki and Mir-Bahador Aryanezhad and Nima Shafii},
  title    = {A hybrid method of fuzzy simulation and genetic algorithm to optimize constrained inventory control systems with stochastic replenishments and fuzzy demand},
