@Article{0270132A-E9C1-4C53-8576-3EA9A0C6385B,
  author   = {Arne F. Meyer and Jan-Philipp Diepenbrock and Frank W. Ohl and Jörn Anemüller},
  title    = {Fast and robust estimation of spectro-temporal receptive fields using stochastic approximations},
  journal  = {Journal of Neuroscience Methods},
  year     = {2015},
  volume   = {246},
  pages    = {119 - 133},
  issn     = {0165-0270},
  abstract = {Background
The receptive field (RF) represents the signal preferences of sensory neurons and is the primary analysis method for understanding sensory coding. While it is essential to estimate a neuron's RF, finding numerical solutions to increasingly complex RF models can become computationally intensive, in particular for high-dimensional stimuli or when many neurons are involved.
New method
Here we propose an optimization scheme based on stochastic approximations that facilitate this task. The basic idea is to derive solutions on a random subset rather than computing the full solution on the available data set. To test this, we applied different optimization schemes based on stochastic gradient descent (SGD) to both the generalized linear model (GLM) and a recently developed classification-based RF estimation approach.
Results and comparison with existing method
Using simulated and recorded responses, we demonstrate that RF parameter optimization based on state-of-the-art SGD algorithms produces robust estimates of the spectro-temporal receptive field (STRF). Results on recordings from the auditory midbrain demonstrate that stochastic approximations preserve both predictive power and tuning properties of STRFs. A correlation of 0.93 with the STRF derived from the full solution may be obtained in less than 10% of the full solution's estimation time. We also present an on-line algorithm that allows simultaneous monitoring of STRF properties of more than 30 neurons on a single computer.
Conclusions
The proposed approach may not only prove helpful for large-scale recordings but also provides a more comprehensive characterization of neural tuning in experiments than standard tuning curves.},
  doi      = {https://doi.org/10.1016/j.jneumeth.2015.02.009},
  keywords = {Sensory coding, Receptive field, Auditory system, Generalized linear model, Stochastic gradient descent, On-line learning},
  url      = {http://www.sciencedirect.com/science/article/pii/S0165027015000618},
}

@Article{270634ED-F373-47D9-8744-9D32B4E44EE3,
  author   = {M. Gitizadeh and J. Aghaei},
  title    = {Dynamic security consideration in multiobjective electricity markets},
  journal  = {Applied Soft Computing},
  year     = {2014},
  volume   = {16},
  pages    = {1 - 9},
  issn     = {1568-4946},
  abstract = {After electricity energy market clearing, the network may be operated with a low transient stability margin because of hitting security limits or increasing the contribution of risky participants. Therefore, a new multi-objective model for electricity energy market clearing, considering dynamic security aspect of the power system, is proposed. Indeed, in addition to the economical aspects of electricity markets, i.e. offer cost of generating units, linearized Corrected Transient Energy Margin (CTEM) of system is also considered as new objective function of market clearing problem. A Multi-Objective Genetic Algorithm (MOGA) is utilized to solve the multi-objective market clearing problem. The New England test system is used to demonstrate the performance of the proposed method.},
  doi      = {https://doi.org/10.1016/j.asoc.2013.11.013},
  keywords = {Electricity energy market clearing, Multi-Objective Genetic Algorithm (MOGA), Corrected Transient Energy Margin (CTEM)},
  url      = {http://www.sciencedirect.com/science/article/pii/S1568494613004122},
}

@Article{283372A5-2070-49A6-B55B-CCEB8E49169D,
  author   = {Jiansheng Guo and Zutong Wang and Mingfa Zheng and Ying Wang},
  title    = {Uncertain multiobjective redundancy allocation problem of repairable systems based on artificial bee colony algorithm},
  journal  = {Chinese Journal of Aeronautics},
  year     = {2014},
  volume   = {27},
  number   = {6},
  pages    = {1477 - 1487},
  issn     = {1000-9361},
  abstract = {Based on the uncertainty theory, this paper is devoted to the redundancy allocation problem in repairable parallel-series systems with uncertain factors, where the failure rate, repair rate and other relative coefficients involved are considered as uncertain variables. The availability of the system and the corresponding designing cost are considered as two optimization objectives. A crisp multiobjective optimization formulation is presented on the basis of uncertainty theory to solve this resultant problem. For solving this problem efficiently, a new multiobjective artificial bee colony algorithm is proposed to search the Pareto efficient set, which introduces rank value and crowding distance in the greedy selection strategy, applies fast non-dominated sort procedure in the exploitation search and inserts tournament selection in the onlooker bee phase. It shows that the proposed algorithm outperforms NSGA-II greatly and can solve multiobjective redundancy allocation problem efficiently. Finally, a numerical example is provided to illustrate this approach.},
  doi      = {https://doi.org/10.1016/j.cja.2014.10.014},
  keywords = {Artificial bee colony algorithm, Multiobjective optimization, Redundancy allocation problem, Repairable systems, Uncertainty theory},
  url      = {http://www.sciencedirect.com/science/article/pii/S100093611400168X},
}

@Article{285492DA-7ED1-4341-8D0D-3691BAD799B0,
  author   = {Y{\i}ld{\i}z, Ali R{\i}za},
  title    = {A novel particle swarm optimization approach for product design and manufacturing},
  journal  = {The International Journal of Advanced Manufacturing Technology},
  year     = {2008},
  volume   = {40},
  number   = {5},
  pages    = {617},
  month    = {Apr},
  issn     = {1433-3015},
  abstract = {This paper presents a novel optimization approach that is a new hybrid optimization approach based on the particle swarm optimization algorithm and receptor editing property of immune system. The aim of the present research is to develop a new optimization approach and then to apply it in the solution of optimization problems in both the design and manufacturing areas. A single-objective test problem, tension spring problem, pressure vessel design optimization problem taken from the literature and two case studies for multi-pass turning operations are solved by the proposed new hybrid approach to evaluate performance of the approach. The results obtained by the proposed approach for the case studies are compared with a hybrid genetic algorithm, scatter search algorithm, genetic algorithm, and integration of simulated annealing and Hooke-Jeeves pattern search.},
  day      = {18},
  doi      = {10.1007/s00170-008-1453-1},
  url      = {https://doi.org/10.1007/s00170-008-1453-1},
}

